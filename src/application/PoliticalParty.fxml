<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SampleController">
   <center>
      <BorderPane prefHeight="373.0" prefWidth="472.0" BorderPane.alignment="CENTER">
         <top>
            <AnchorPane prefHeight="375.0" prefWidth="472.0" BorderPane.alignment="CENTER">
               <children>
                  <Label layoutX="31.0" layoutY="65.0" text="NEW POLITICAL PARTY REGISTRATION FORM">
                     <font>
                        <Font name="System Bold" size="19.0" />
                     </font>
                  </Label>
                  <Label layoutX="51.0" layoutY="139.0" text="Party Name:">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Label>
                  <Label layoutX="51.0" layoutY="199.0" text="Leader Name:">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Label>
                  <TextField fx:id="pname" layoutX="172.0" layoutY="137.0" onAction="#pnamet" promptText="enter party name" />
                  <TextField fx:id="lname" layoutX="172.0" layoutY="197.0" onAction="#lnamet" promptText="enter leader name" />
                  <Label layoutX="49.0" layoutY="266.0" text="Symbol Name:">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Label>
                  <TextField fx:id="symbol" layoutX="172.0" layoutY="264.0" onAction="#symbolt" promptText="enter symbol name" />
                  <Button fx:id="processinformation" layoutX="162.0" layoutY="315.0" mnemonicParsing="false" onAction="#pinformationbutton" text="PROCESS INFORMATION" />
                 
               </children>
            </AnchorPane>
         </top>
      </BorderPane>
   </center>
</BorderPane>
